<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AuthInput">
        <attr name="inputType">
            <enum name="text" value="1" />
            <enum name="password" value="129" />
            <enum name="email" value="33" />
            <enum name="phone" value="3" />
            <enum name="number" value="2" />
        </attr>
        <attr name="labelText" format="string" />
        <attr name="hintText" format="string" />
    </declare-styleable>
    <declare-styleable name="AuthButton">
        <attr name="authButtonText" format="string" />
        <attr name="authButtonIcon" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="BackButton">
        <attr name="backButtonText" format="string" />
    </declare-styleable>
</resources>